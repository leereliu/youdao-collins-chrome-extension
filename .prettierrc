{
  "semi": false,
  "singleQuote": false,
  "trailingComma": "none",
  "tabWidth": 2,
  "printWidth": 80,
  "plugins": ["@plasmohq/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^plasmo$",
    "<THIRD_PARTY_MODULES>",
    "^~(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
